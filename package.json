{
  "name": "dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "##### BUILD #########": "###################################",
    "build": "tsc && vite build",
    "build:test": "tsc --noEmit",
    "##### DEV ###########": "###################################",
    "dev": "vite",
    "dev:preview": "vite preview",
    "##### TEST ##########": "###################################",
    "test": "vitest run",
    "test:watch": "vitest watch --ui",
    "test:coverage": "vitest run --coverage",
    "##### LINT ##########": "###################################",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:style:fix && npm run lint:prettier:fix",
    "lint:staged": "lint-staged",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix --ext .ts,.tsx .",
    "lint:style": "stylelint \"src/**/*.{css,ts}\"",
    "lint:style:fix": "stylelint --fix \"src/**/*.{css,ts}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json,svg,html}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json,svg,html}\"",
    "##### NPM ###########": "###################################",
    "prepare": "husky"
  },
  "dependencies": {
    "@adobe/lit-mobx": "2.2.2",
    "@axa-fr/oidc-client": "7.22.4-alpha.1423",
    "@lit-labs/router": "0.1.3",
    "@shoelace-style/shoelace": "2.15.0",
    "date-fns": "3.6.0",
    "lit": "3.1.3",
    "marked": "12.0.2",
    "mobx": "6.12.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.0.2",
    "@eslint/js": "9.1.1",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/coverage-v8": "1.5.0",
    "eslint": "8.57.0",
    "eslint-define-config": "2.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss-lit": "1.1.1",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "stylelint": "16.4.0",
    "stylelint-config-recess-order": "5.0.1",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.7.1",
    "vite": "5.2.9",
    "vite-plugin-static-copy": "1.0.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.5.0"
  },
  "volta": {
    "node": "20.12.2",
    "npm": "10.5.2"
  }
}
